{
  "templateName": "starter-typescript-project",
  "pinkyringVersion": "1.0.0",
  "removableOptions": [
    {
      "label": "DOCKER",
      "globPatterns": [
        ".dockerignore",
        "Dockerfile"
      ],
      "contentPattern": ".pinkyring=DOCKER",
      "scriptNames": [
        "docker:build",
        "docker:run",
        "docker:stop"
      ]
    },
    {
      "label": "POSTGRES & TYPEORM",
      "globPatterns": [
        "src/entities/demoEntity.ts",
        "src/relationalDB"
      ],
      "contentPattern": ".pinkyring=POSTGRES",
      "packageNames": [
        "pg",
        "typeorm",
        "typeorm-extension"
      ],
      "scriptNames": [
        "db:create",
        "db:seed",
        "db:migration:generate",
        "db:migration:run",
        "db:drop"
      ]
    }
  ]
}