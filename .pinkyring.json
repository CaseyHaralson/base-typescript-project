{
  "templateName": "starter-typescript-project",
  "pinkyringVersion": "1.0.0",
  "removableOptions": [
    {
      "label": "DOCKER",
      "globPatterns": [
        ".dockerignore",
        "Dockerfile"
      ],
      "contentPattern": ".pinkyring=DOCKER"
    },
    {
      "label": "POSTGRES & TYPEORM",
      "globPatterns": [
        "src/entities/demoEntity.ts",
        "src/relationalDB"
      ],
      "contentPattern": ".pinkyring=POSTGRES",
      "packageNames": [
        "typeorm",
        "typeorm-extension"
      ]
    }
  ]
}